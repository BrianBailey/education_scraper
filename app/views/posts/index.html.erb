
<iframe width="600" height="371" seamless frameborder="0" scrolling="no" src="https://docs.google.com/spreadsheets/d/15_VtSC_S0GLwUIz-h9ScRZRG54gcDmyV-lhxwdHHrsw/pubchart?oid=1389393926&amp;format=interactive"></iframe>


<div class="container">

<table class="table table-hover table-condensed">
  <thead>
    <tr>
      <th>speaker state</th>
      <th>speaker first</th>
      <th>speaker last</th>
      <th>speaker party</th>
      <th>speaking</th>
      <th>chamber</th>
      <th>title</th>
      <th>date</th>
      <th>origin_url</th>
      <th>number</th>
      <th>order</th>
      <th>volume</th>
      <th>pages</th>
      <th>bioguide_id</th>
      <th>speaker_raw</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.speaker_state %></td>
        <td><%= post.speaker_first %></td>
        <td><%= post.speaker_last %></td>
        <td><%= post.speaker_party %></td>
        <td><%= post.speaking %></td>
        <td><%= post.chamber %></td>
        <td><%= post.title %></td>
        <td><%= post.date %></td>
        <td><%= post.origin_url %></td>
        <td><%= post.number %></td>
        <td><%= post.order %></td>
        <td><%= post.volume %></td>
        <td><%= post.pages %></td>
        <td><%= post.bioguide_id %></td>
        <td><%= post.speaker_raw %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<%= will_paginate @posts, renderer: BootstrapPagination::Rails %>

<br>

<%= link_to 'New Post', new_post_path %>
</div>
